  div.row
    div.col-md-2 10:30 am
    each talk in talks.dayOne[1]
      div.col-md-2
        a(href="#",data-toggle="modal",data-target="##{talk.uniqueId}") !{talk.title} (!{talk.speakerName})
        div(class="modal fade",id= talk.uniqueId,tabindex="-1",role="dialog")
          div(class="modal-dialog")
            div(class="modal-content")
              div(class="modal-header")
                button(type="button",class="close",data-dismiss="modal") &times;
                h4(class="modal-title") #{talk.title}&nbsp;
                  small by #{talk.speakerName}

              div(class="modal-body")
                !{talk.abstractHtml}
              div(class="modal-footer")
                button(type="button",class="btn btn-default",data-dismiss="modal") Close

